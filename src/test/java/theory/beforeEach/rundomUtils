public class RandomUtilsExamples {
    public static void main(String[] args){
        System.out.print("Random string of 10 letters: " + getRandomString(10));
        System.out.print("Random int between 1234 and 999999: " + getRandomInt(1234, 999999));
        System.out.print("Random email: " + getRandomEmail());
    }
}